                </navigator>
                <navigator url="../committee/committee" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                    <view class="weui-cell__hd"><image src="../../../images/committe.png" style="width: 20px;height: 20px;margin-right: 5px" /></view>
