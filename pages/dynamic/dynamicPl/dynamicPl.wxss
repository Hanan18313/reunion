/* pages/dynamic/dynamicPl/dynamicPl.wxss */
@import '../../../style/weui.wxss';
page,
.page,
.page__bd{
    height: 100%;
}
.img{
  height: 40%;
  display: flex;
  flex-wrap: wrap;
  margin: 0 100rpx;
}
.img view{
  width: 33%;
}
.img view image{
  width: 100%;
  height: 180rpx;
}
.title view{
  /* background:#f4f4f4; */
  padding: 20rpx 60rpx;
}


/**index.wxss**/
.button{
  position: absolute;
  bottom: 0;
  width: 100%;
}
.aon{
position: absolute;
white-space:nowrap;/* 防止向下换行*/
}
.doommview{
  z-index: 3;
  height: 18%;
  width: 100%;
  position: absolute;
}
/**定义从右边向左边的移动的动画**/
@keyframes first{
  from{left: 100%; }
  to{left: -100%;}
}
